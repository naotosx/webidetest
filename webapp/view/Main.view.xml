<mvc:View controllerName="git.test.WebIDETestGit.controller.Main" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Text text="Mix Reset"/>
					<Button text="Ok"/>
					<Panel>
						<Input width="20%" placeholder="Ingrese usuario"/>
						<Button text="Ok" press="onOk" type="Reject"/>
						<NavigationList id="navigationList" width="320px" selectedItem="subItemThree">
							<NavigationListItem text="Item 1" icon="sap-icon://employee">
								<NavigationListItem text="Sub Item 1"/>
								<NavigationListItem text="Sub Item 2"/>
								<NavigationListItem text="Sub Item 3" id="subItemThree"/>
								<NavigationListItem text="Sub Item 4"/>
								<NavigationListItem text="Invisible Sub Item 5" visible="false"/>
								<NavigationListItem text="Invisible Sub Item 6" visible="false"/>
							</NavigationListItem>
						</NavigationList>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>